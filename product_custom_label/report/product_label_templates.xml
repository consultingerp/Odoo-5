<?xml version="1.0" encoding="UTF-8"?>
<odoo>

    <template id="label_57x35">
        <table class="table table-bordered col-xs-4">
            <tr height="155px" style="border:0;margin:0;padding:0;">
                <td class="col-xs-4 text-center" style="border:0;margin:0;padding:0;">
                    <div style="overflow:hidden; width: 252px !important; padding: 5px 5px 0; line-height: 1.3;">
                        <span style="font-size: 18px;" t-field="label.product_id.name"/>
                        <span t-if="label.product_id.attribute_value_ids" t-esc="u', '.join(map(lambda x: x.attribute_id.name + u': ' + x.name, label.product_id.attribute_value_ids))"/>
                    </div>

                    <div style="width: 100%; padding: 0; margin: 0; background-color: #f8fb46;border: 2px solid #000;font-size:12px;">
                        <div style="width: 10%;display:inline-block;">
                            <span>Our Price</span>
                        </div>
                        <div style="width: 60%;display:inline-block;font-size: 28px;font-weight:bold;">
                            <span t-esc="label.product_id.currency_id.symbol"/>
                            <span t-esc="label.product_id.list_price"/>
                        </div>
                        <div style="width: 20%;display:inline-block;">
                            <span class="text-center">6.5 OZ - 1.20 per OZ</span>
                        </div>
                    </div>

                    <div t-if="label.product_id.default_code" style="width: 100%; padding: 0; margin: 0; font-size: 12px;">
                        <span t-field="label.product_id.default_code"/>
                    </div>
                    <div t-if="label.product_id.barcode" style="width: 100%; padding: 0; margin: 0; font-size: 12px;">
                        <span t-esc="label.product_id.barcode"/>
                    </div>
                    <div t-if="label.product_id.barcode" class="text-center align-middle" style="margin: 0; padding: 0 3px; height: 20px;">
                        <t t-if="label.wizard_id.humanreadable" t-set="show_code" t-value="1"/>
                        <t t-else="" t-set="show_code" t-value="0" />
                        <img alt="Barcode"
                             t-att-src="'/report/barcode/?type=%s&amp;value=%s&amp;width=%s&amp;height=%s&amp;humanreadable=%s' % ('Code128', label.product_id.barcode, 600, 100, show_code)"
                             style="overflow: hidden; width: 100%; height: 100%;"/>
                    </div>
                    <div class="col-12" style="height:35px;margin-top:15px;">
                        <t t-set="color1" t-value="docs['product_tag'].color_label_attr_1"/>
                        <t t-set="color2" t-value="docs['product_tag'].color_label_attr_2"/>
                        <t t-set="color3" t-value="docs['product_tag'].color_label_attr_3"/>
                        <t t-set="color4" t-value="docs['product_tag'].color_label_attr_4"/>
                        <t t-set="color5" t-value="docs['product_tag'].color_label_attr_5"/>
                        <span t-if="docs['product_tag'].is_label_attr_1 and label.product_id.label_attr_1"
                              t-attf-style="background-color: {{color1}};border-radius: 50%;padding: 5px;font-size: 12px;font-weight: bold;margin-right:5px;">
                            <t t-esc="label.product_id.label_attr_1"/>
                        </span>
                        <span t-if="docs['product_tag'].is_label_attr_2 and label.product_id.label_attr_2"
                              t-attf-style="background-color: {{color2}};border-radius: 50%;padding: 5px;font-size: 12px;font-weight: bold;margin-right:5px;">
                            <t t-esc="label.product_id.label_attr_2"/>
                        </span>
                        <span t-if="docs['product_tag'].is_label_attr_3 and label.product_id.label_attr_3"
                              t-attf-style="background-color: {{color3}};border-radius: 50%;padding: 5px;font-size: 12px;font-weight: bold;margin-right:5px;">
                            <t t-esc="label.product_id.label_attr_3"/>
                        </span>
                        <span t-if="docs['product_tag'].is_label_attr_4 and label.product_id.label_attr_4"
                              t-attf-style="background-color: {{color4}};border-radius: 50%;padding: 5px;font-size: 12px;font-weight: bold;margin-right:5px;">
                            <t t-esc="label.product_id.label_attr_4"/>
                        </span>
                        <span t-if="docs['product_tag'].is_label_attr_5 and label.product_id.label_attr_5"
                              t-attf-style="background-color: {{color5}};border-radius: 50%;padding: 5px;font-size: 12px;font-weight: bold;margin-right:5px;">
                            <t t-esc="label.product_id.label_attr_5"/>
                        </span>
                    </div>
                </td>
            </tr>
        </table>
    </template>

    <template id="report_product_label_57x35_template">
        <t t-call="web.html_container">
            <t t-call="web.basic_layout">
                <t t-set="count" t-value="0" />
                <div class="page">
                    <div class="oe_structure"/>
                    <t t-set="tag" t-value="docs['product_tag']" />
                    <t t-foreach="docs['labels']" t-as="label">
                        <t t-set="qty" t-value="1" />
                        <t t-if="label.qty">
                            <t t-set="qty" t-value="label.qty" />
                        </t>
                        <t t-foreach="list(range(qty))" t-as="index_qty">
                            <div t-if="count % 21 == 0" style="page-break-after:always;"/>
                            <div t-if="count % 3 == 0" style="clear:both;"/>
                            <div style="width: 30%; float: left; height: 180px; margin: 0 10px;">
                                <t t-call="product_custom_label.label_57x35"/>
                            </div>
                            <t t-set="count" t-value="count+1" />
                        </t>
                    </t>
                </div>
            </t>
        </t>
    </template>

